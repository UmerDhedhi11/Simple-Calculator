num1 = int(input("Enter number: "))
num2 = int(input("Enter number: "))
op = input("Enter Operator: ")

if(op == "+"):
    print(num1+num2)
    
elif(op == "-"):
    print(num1-num2)
    
elif(op == "x","*"):
    print(num1*num2)
    
elif(op == "/","%"):
    print(num1%num2)
    
else:
    print("Invalid Operation")
